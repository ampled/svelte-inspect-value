{
  "name": "svelte-inspect-value",
  "description": "Svelte value inspector component",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ampled/svelte-inspect-value.git"
  },
  "homepage": "https://inspect.eirik.space",
  "license": "MIT",
  "keywords": [
    "svelte",
    "inspect",
    "debug",
    "json",
    "tree"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "build-ele": "vite -c ./vite.config.element.ts build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "eslint": "eslint .",
    "test:unit": "vitest",
    "test:ui": "npm run test:unit -- --ui",
    "test": "npm run test:unit -- --run",
    "optimize": "vite optimize --force"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.28.6"
  },
  "devDependencies": {
    "@docsearch/css": "^3.9.0",
    "@docsearch/js": "^3.9.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.21.0",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@testing-library/user-event": "^14.6.1",
    "brace": "^0.11.1",
    "colord": "^2.9.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.0.0",
    "happy-dom": "^17.1.0",
    "mdsvex": "^0.12.5",
    "p-memoize": "^7.1.1",
    "prettier": "^3.4.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-svelte": "^3.3.2",
    "publint": "^0.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-heading-id": "^1.0.1",
    "rxjs": "^7.8.2",
    "semantic-release": "^24.2.2",
    "shiki": "^2.1.0",
    "svelte": "^5.28.6",
    "svelte-check": "^4.1.7",
    "svelte-eslint-parser": "^1.1.3",
    "svelte-preprocess": "^6.0.3",
    "terser": "^5.36.0",
    "typedoc": "^0.28.2",
    "typedoc-plugin-frontmatter": "^1.3.0",
    "typedoc-plugin-markdown": "^4.6.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "6.3.5",
    "vite-plugin-devtools-json": "^0.1.0",
    "vitest": "3.1.3"
  },
  "dependencies": {
    "esm-env": "^1.2.2",
    "fast-deep-equal": "^3.1.3",
    "highlight.js": "^11.10.0",
    "many-keys-map": "^2.0.1",
    "memoize": "^10.1.0"
  }
}
