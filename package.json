{
  "name": "svelte-inspect-value",
  "version": "0.0.2",
  "repository": {
    "url": "git+https://github.com/ampled/svelte-inspect-value.git"
  },
  "homepage": "https://svelte-inspect-value.vercel.app/",
  "license": "MIT",
  "keywords": [
    "svelte",
    "inspect",
    "debug",
    "json",
    "tree"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "build-ele": "vite -c ./vite.config.element.ts build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "eslint": "eslint .",
    "test:unit": "vitest",
    "test:ui": "npm run test:unit -- --ui",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.19.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.9.1",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/ui": "^3.0.0-beta.4",
    "colord": "^2.9.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.0.0",
    "happy-dom": "^16.5.3",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-svelte": "^3.3.2",
    "publint": "^0.2.0",
    "svelte": "^5.19.0",
    "svelte-check": "^4.1.1",
    "svelte-eslint-parser": "^1.0.0-next.4",
    "svelte-highlight": "^7.8.2",
    "svelte-preprocess": "^6.0.3",
    "terser": "^5.36.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "6.0.11",
    "vitest": "3.0.0-beta.4"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "highlight.js": "^11.10.0"
  }
}
