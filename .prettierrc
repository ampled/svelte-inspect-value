{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": false,
  "printWidth": 100,
  "organizeImportsSkipDestructiveCodeActions": true,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
